public class LacoDeRepeticao {
	public static void main(String[] args) {

		// laço while
		int i = 100;
		while(i < 10) {
			System.out.print(i +", ");
			i++;
		}
		
		System.out.println("\nestou fora do laco while");
		
		// laço do/while
		int k = 100;
		do {
			System.out.print(k +", ");
			k++;
		} while(k < 10);
		
		System.out.println("\nestou fora do laco do/while");
	}
}