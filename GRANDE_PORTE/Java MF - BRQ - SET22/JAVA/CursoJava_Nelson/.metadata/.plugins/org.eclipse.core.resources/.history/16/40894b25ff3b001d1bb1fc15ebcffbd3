public class TestaPessoa {
	public static void main(String[] args) {
		Pessoa [] povo = new Pessoa[5];
		
		povo[0] = new Pessoa(123,"Jose Pereira", 1.65);
		povo[1] = new Pessoa(754,"Ana Lee", 1.8);
		povo[2] = new Pessoa(874,"Denis Oliveira", 1.5);
		povo[3] = new Pessoa(694,"Augusto Viana", 1.7);
		povo[4] = new Pessoa(458,"Maria Costa", 1.65);
		
		// double media = Math.floor(Pessoa.mediaAltura(povo)*100.0)/100.0;
		double media = Math.round(Pessoa.mediaAltura(povo));
		
		System.out.println("Altura media = " + media);
	}

}
