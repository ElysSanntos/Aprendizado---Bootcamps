public class LacoDeRepeticao {
	public static void main(String[] args) {

		// laço enquanto
		int i = 0;
		while(i < 10) {
			System.out.print(i +", ");
			i++;
		}
		
		System.out.println("\nestou fora do laco enquanto");
		
		// laço faça enquanto
		int k = 0;
		do {
			System.out.print(k +", ");
			k++;
		} while(k < 10);
		
		System.out.println("\nestou fora do laco faca enquanto");
	}
}