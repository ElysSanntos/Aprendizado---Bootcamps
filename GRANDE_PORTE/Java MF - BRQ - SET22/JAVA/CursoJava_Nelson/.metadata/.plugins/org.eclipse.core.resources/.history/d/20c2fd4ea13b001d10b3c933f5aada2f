import java.util.ArrayList;

public class TestaArrayList {

	public static void main(String[] args) {
		ArrayList cornucopia = new ArrayList();
		
		cornucopia.add(5);
		System.out.println(cornucopia);
		cornucopia.add(3.1);
		System.out.println(cornucopia);
		cornucopia.add(0,"ana");
		System.out.println(cornucopia);
	}

}
