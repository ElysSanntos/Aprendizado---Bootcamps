
public class TestaVeiculo {
	public static void main(String[] args) {
		try {
			Veiculo fusca = new Veiculo();
			System.out.println("velocidade atual " + fusca.getVelocidade());
			fusca.acelera(50);
			System.out.println("velocidade atual " + fusca.getVelocidade());
			fusca.desacelara(20);
			System.out.println("velocidade atual " + fusca.getVelocidade());
		} catch (Exception e) {
			System.out.println(e);
		}

	}
}
