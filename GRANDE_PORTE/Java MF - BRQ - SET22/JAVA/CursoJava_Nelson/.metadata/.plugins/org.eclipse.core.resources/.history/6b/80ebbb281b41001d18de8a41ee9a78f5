public class SubClasse extends SuperClasse {
	// construtor sem parâmetro
	public SubClasse() {
		System.out.println("Construtor SubClasse sem parametro");
	}
	
	// construtor com parâmetro
	public SubClasse(int a) {
		super(a);
		System.out.println("Construtor SubClasse com parametro => " + a);
	}	
}