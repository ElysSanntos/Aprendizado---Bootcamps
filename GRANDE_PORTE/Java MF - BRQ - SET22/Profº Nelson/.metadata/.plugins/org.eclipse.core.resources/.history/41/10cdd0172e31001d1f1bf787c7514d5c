public class Cliente {
	// atributos
	public int    idCliente;
	public String nomeCliente;
	public String cpfClinte;
	// valor default
	public double limiteCliente = 5000.0;
	
	// mÃ©todos
	public void exibeLimite() {
		System.out.println("limite = " + limiteCliente);
		if(limiteCliente > 10000) {
			viva();
		}
	}
	
	private void viva() {
		System.out.println("Viva... parabens!");
	}
}