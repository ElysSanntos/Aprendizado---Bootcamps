public class P1E02 {
	public static void main(String[] args) {
		int q1   = 1;
		int q2   = 1;
		int q3   = 1;
		int q4   = 1;
		int q5   = 1;
		int q6   = 1;
		double D =  1.0;
		double R = (q1 
				   +q2 * 0.50 
				   +q3 * 0.25 
				   +q4 * 0.10
				   +q5 * 0.05 
				   +q6 * 0.01) * D;
		System.out.println("reais = R$"+R);
	}
}